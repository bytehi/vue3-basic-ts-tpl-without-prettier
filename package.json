    "@iconify-json/ep": "^1.1.7",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.1.0",
    "commitizen": "^4.2.5",
    "commitlint-config-cz": "^0.13.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.24.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
